{
    "compilerOptions": {
        "target": "ES6",
        "jsx": "react-jsx",
        "module": "ES6",
        "moduleResolution": "Bundler",
        "noEmit": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": [
        "*.ts",
        "*.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}
